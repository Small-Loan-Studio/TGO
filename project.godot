boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

Signalbus="*res://Scripts/Signalbus.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[debug]

gdscript/warnings/untyped_declaration=2

[dialogic]

