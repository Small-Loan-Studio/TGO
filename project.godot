directories/dtl_directory={
"Devin_wakeup": "res://ZZ_Scratch/QuestPrototype/Timelines/Devin_wakeup.dtl",
"coffin_0": "res://ZZ_Scratch/QuestPrototype/Timelines/coffin_0.dtl",
"found_lamp": "res://ZZ_Scratch/QuestPrototype/Timelines/found_lamp.dtl",
